
#how create new Repository
#create Folder 
mkdir giovanniltWEB

cd giovanniltWEB

git config --global user.name "Giovanni La Torre"
git config --global user.email giovannilt@gmail.com

git init

git remote add origin git@github.com:giovannilt/WEBgiovannilt.git
git push origin master


#hot check out 

git clone git@github.com:giovannilt/WEBgiovannilt.git
